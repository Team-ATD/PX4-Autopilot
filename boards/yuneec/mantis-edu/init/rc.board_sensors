#!/bin/sh
#
# board specific sensors init
#------------------------------------------------------------------------------

board_adc start

# Internal SPI bus ICM-20602
icm20602 start -b 1 -R 4 -s start

# External I2C bus
#ist8310 -C -R 4 start
ist8310 start -I -b 2

# Baro on internal I2C
mpc2520 -I start

gps start -d /dev/ttyS0 -p ubx -b 115200
